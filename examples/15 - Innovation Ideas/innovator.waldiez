    "skills": [
      {
        "id": "ws-174722995042744roqOwqTAhlu4-yb8xiN",
        "type": "skill",
        "name": "get_papers",
        "description": "Use this function to retrieve papers from arxiv. The args are search_query (str): Name of topic of interest (e.g., 'agents') max_returns (int): number of papers returned",
        "tags": [],
        "requirements": [
          "arxiv"
        ],
        "createdAt": "2025-05-14T13:39:10.427Z",
        "updatedAt": "2025-05-14T13:58:51.951Z",
        "data": {
          "content": "def get_papers(search_query: str, max_results: int) ->  Annotated[str, \"list of papers\"]:\n    \"\"\"\n    return list of papers based on the user topic.\n    \n    Args:\n        search_query (str): Name of topic of interest (e.g., 'agents')\n        max_returns (int): number of papers returned\n    \"\"\"\n    import arxiv\n\n    results = []\n\n    # Search for recent papers on the topic\n    search = arxiv.Search(\n        query=search_query,\n        max_results=max_results,\n        sort_by=arxiv.SortCriterion.SubmittedDate,\n        sort_order=arxiv.SortOrder.Descending\n    )\n\n    # Retrieve and print the paper details\n    for result in search.results():\n        print(f\"Title: {result.title}\")\n        print(f\"Authors: {', '.join(author.name for author in result.authors)}\")\n        print(f\"Published: {result.published}\")\n        print(f\"Summary: {result.summary}\")\n        print(f\"PDF URL: {result.pdf_url}\\n\")\n        results.append(result.summary)\n\n    return results\n    \n",
          "skillType": "custom",
          "secrets": {}
        }
      }
    ],
